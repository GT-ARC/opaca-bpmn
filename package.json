{
  "name": "bpmn-js-interpreter",
  "version": "0.0.0",
  "description": "A bpmn-js modeler with properties panel and token-simulator and some extensions for interpretation",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s build",
    "dev": "run-s build:less watch:js",
    "start": "run-s build serve",
    "serve": "sirv public --dev",
    "build": "run-s build:*",
    "build:less": "lessc src/app.less public/app.css",
    "build:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"build:js -- --watch\"",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "",
    "url": "",
    "directory": ""
  },
  "keywords": [
    "bpmnjs-example",
    "bpmn-js-properties-panel",
    "bpmn-js-token-simulation",
    "business-process-interpretation"
  ],
  "author": {
    "name": "",
    "url": ""
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/extract-process-variables": "^0.8.0",
    "@bpmn-io/properties-panel": "^3.4.0",
    "bpmn-js": "^15.1.3",
    "bpmn-js-properties-panel": "^5.0.0",
    "bpmn-js-token-simulation": "^0.32.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "htm": "^3.1.1",
    "ids": "^1.0.5",
    "jquery": "^3.5.1",
    "min-dash": "^4.0.0",
    "min-dom": "4.1.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "less": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^2.0.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^5.0.0"
  }
}
